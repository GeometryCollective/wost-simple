Point a, b
Let ab := Segment(a,b)
IsOriented( ab )

Point c, d
Let cd := Segment(c,d)
IsOriented( cd )

Point x
Let xab := Triangle(x,a,b)
Let xcd := Triangle(x,c,d)
Angle tab := InteriorAngle(x,a,b)
Angle tcd := InteriorAngle(x,c,d)
IsOriented( tab )
IsOriented( tcd )

Label a $a$
Label b $b$
Label x $x$
